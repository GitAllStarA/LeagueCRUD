<title><% content_for(:title,"Create New player") %></title>
<h1>Create new players</h1>



<%= render partial: "players/form" %>

<%
=begin%>

<form action="/players" method="POST">

    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token%>">

    <p>
        <label>first name &nbsp;</label>
        <input type="text" name="player[first_name]">
    </p>

    <p>
        <label>last name &nbsp;</label>
        <input type="text" name="player[last_name]">
    </p>

    <p>
        <input type="submit" value="create">
    </p>

</form>
<%
=end%>

<%= link_to  "Go Back to players!", players_path %>